<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".addPlaceActivity">

    <Button
        android:id="@+id/viewPlaceList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="263dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="57dp"
        android:text="View Places"
        app:layout_constraintBottom_toTopOf="@+id/addNewPlace"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addNewPlace"
        android:layout_width="112dp"
        android:layout_height="44dp"
        android:layout_marginStart="163dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="319dp"
        android:text="Add Place"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewPlaceList" />

</androidx.constraintlayout.widget.ConstraintLayout>